<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Title>Vite AspNetCore integration</Title>
        <Authors>Victor Gladkikh</Authors>
        <Description>Vite integration helper to use with existing applications. 
This library is agnostic to the underlying js framework.</Description>
        <PackageProjectUrl>https://github.com/alfeg/vite-aspnetcore</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/alfeg/vite-aspnetcore/blob/main/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/alfeg/vite-aspnetcore</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>vite spa middleware</PackageTags>
        <OutputType>Library</OutputType>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Yarp.ReverseProxy" Version="2.0.0-rc.1.23068.3" />
    </ItemGroup>

</Project>
